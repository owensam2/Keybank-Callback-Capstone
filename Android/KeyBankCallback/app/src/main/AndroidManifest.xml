<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mmarf.keybankcallback">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CallbackActivity" />
        <activity android:name=".CallbackQuestionsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CallbackConformationActivity" />
        <activity
            android:name=".CallbackScheduleActivity"
            android:label="@string/title_activity_callback_schedule" />
        <activity android:name=".CallbackActivityNoWait" />
        <activity android:name=".CallbackSuggestionScheduler" />
        <activity android:name=".CallbackConformationTimeActivity" />
        <activity android:name=".CallbackScheduleActivityTime"></activity>
    </application>

</manifest>